int parens(int num, int open, int close) {

	if(open + close == num) return;
	
	if(num == 0) {
		print('(');
		parens(num, open + 1, close);
	}

	if(open < num) {
		print(')');
		parens(num, open, close + 1);
		if(open + 1 < num) {
			print('(');
			parens(num, open + 1, close);
		}
	}
	
	print('\n');
}

