int sumTri(int array[], int count, int ans[], int ansindex) {
    if(array.length<=1){
        return {};
    }
    if(count <= arr.length-1){
        ans[ansindex] = array[count]+array[count-1];
        sumTri(array, count+1, ans, ansindex+1);

    }
    else{
        int [] newans = new int[];
        int arrlen = array.length;

        int [] final = sumTri(ans, 0, newans);
        int [] result = new int[final.length + arr.len]
        System.arraycopy(array, 0, result, arrlen);
        System.arraycopy(final, 0, result , anslen);
        return result;
    }
    }
	
}

